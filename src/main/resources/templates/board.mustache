{{>layout/header}}
{{>layout/nav}}
{{>layout/userLoginBar}}

<div class="jb-wrap">
    <div class="jb-image"><img src="img/base.png" alt=""></div>
    <div class="jb-text">
        <p>#자유게시판</p>
    </div>
</div>


<div class="table-container">
    <div>
        <div>

            <table>
                <thead>
                <tr>
                    <th>글 번호</th>
                    <th>제 목</th>
                    <th>작성자</th>
                    <th>최종수정일</th>
                </tr>
                </thead>


                <tbody id="tbody">
                {{#board}}
                    <tr>
                        <td>{{id}}</td>
                        <td><a href="/boardRead/{{id}}">{{title}}</a></td>
                        <td>{{writer}}</td>
                        <td>{{modifiedDate}}</td>
                    </tr>
                {{/board}}
                </tbody>
            </table>
            <div class="table-btn1">
                <ul class="pagination">
                    {{#goPreviousPage}}
                        <li class="page-item"><a class="page-link" href="/board?page={{.}}">Previous</a></li>
                    {{/goPreviousPage}}
                    {{#previousPageList}}
                        <li class="page-item"><a class="page-link" href="/board?page={{a}}">{{b}}</a></li>
                    {{/previousPageList}}
                    <li class="page-item active"><a class="page-link" href="/board?page={{currentPageA}}">{{currentPage}}</a></li>
                    {{#nextPageList}}
                        <li class="page-item"><a class="page-link" href="/board?page={{a}}">{{b}}</a></li>
                    {{/nextPageList}}
                    {{#goNextPage}}
                        <li class="page-item"><a class="page-link" href="/board?page={{.}}">Next</a></li>
                    {{/goNextPage}}
                </ul>
            </div>

            <div class="table-btn"><a href="/boardWrite">글쓰기</a></div>
        </div>
    </div>
</div>


{{>layout/footer}}
